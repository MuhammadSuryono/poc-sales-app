# Environment Variables
# Copy this file to .env and fill in your values

# ImageKit Configuration
NUXT_PUBLIC_IMAGEKIT_PUBLIC_KEY=your_public_key_here
NUXT_PUBLIC_IMAGEKIT_PRIVATE_KEY=your_private_key_here
NUXT_PUBLIC_IMAGEKIT_URL_ENDPOINT=your_url_endpoint_here

# OpenCage Geocoding API
NUXT_PUBLIC_OPENCAGE_API_KEY=your_opencage_api_key_here

# API Base URL
NUXT_PUBLIC_API_BASE_URL=http://localhost:3000/api

# App Configuration
NUXT_PUBLIC_APP_NAME=JAVA.co Sales App
NUXT_PUBLIC_APP_VERSION=1.0.0
